# LeetCode API Configuration
LEETCODE_GRAPHQL_URL=https://leetcode.com/graphql

# Output Configuration
OUTPUT_DIR=./output
PROBLEM_NAME=LeetCode

# Input Files
COOKIES_FILE=./config/cookies.json
PROBLEMS_FILE=./config/problems.txt

# Output Mode
# Set to 'true' to create separate files for each problem
# Set to 'false' to append all problems to a single file
SEPARATE_FILES=false

# Delay between requests (milliseconds) to avoid rate limiting
REQUEST_DELAY=1000